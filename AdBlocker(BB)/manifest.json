{
    "manifest_version": 3,
    "name": "Simple AdBlocker",
    "version": "1.0",
    "description": "A simple Chrome ad blocker extension using declarativeNetRequest",
    "permissions": [
        "declarativeNetRequest",
        "storage"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "host_permissions": [
        "*://*/*"
    ]
}
